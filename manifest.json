{
    "manifest_version": 3,
    "name": "BOT Dev Tools Helper",
    "version": "0.2.0",
    "description": "Personal dev helper tools for BlackOre APIs.",
    "action": {
      "default_title": "BOT Dev Tools Helper",
      "default_popup": "popup.html"
    },
    "permissions": ["storage", "tabs"],
    "host_permissions": ["<all_urls>"],
    "icons": {
      "16": "icons/16x16.png",
      "32": "icons/32x32.png",
      "48": "icons/48x48.png",
      "128": "icons/167x167.png"
    }
  }
  